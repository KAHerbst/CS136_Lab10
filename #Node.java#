hexboard and player
//white is moving first

public class Node{

    //player or color
    char k;
    HexBoard value;
    Vector<Node> children;
    
    //if 'o' --> white
    //if '*' --> black
    Node(HexBoard board, char k){
	this.value = board;
	this.children = this.lineage();
	this.color = k;
    }

    public Vector<Node> lineage(){
	Vector<HexMoves> moves = this.value.moves(this.opponent(this.color));	
    }
}

